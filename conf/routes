# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET        /                    controllers.Tweets.index

GET        /odin                controllers.Tweets.odinindex

GET        /login               controllers.Tweets.login()
POST       /login               controllers.Tweets.authenticate()

GET        /logout              controllers.Tweets.logout()


# Use `Assets.versioned` to enable Play 2.3's Asset Fingerprinting
GET        /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)